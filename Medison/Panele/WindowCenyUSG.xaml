<Window x:Class="WindowCenyUSG"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="WindowCenyUSG" Height="320" Width="548" WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid AutoGenerateColumns="False" Margin="6,46,10,7" Name="DataGrid1" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="NrUsg" Binding="{Binding Path=NrUsg}"/>
                <DataGridTextColumn Header="CzyjeUsgSkrot" Binding="{Binding Path=CzyjeUsgSkrot}"/>
                <DataGridTextColumn Header="CzyjeUsg" Binding="{Binding Path=CzyjeUsg}"/>
                <DataGridTextColumn Header="KwotaUsg" Binding="{Binding Path=KwotaUsg}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Zapisz" Height="25" HorizontalAlignment="Left" Margin="16,8,0,0" Name="Button1" VerticalAlignment="Top" Width="143" />
                
    </Grid>
</Window>
